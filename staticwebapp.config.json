{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "mimeTypes": {
    ".json": "application/json"
  },
  "globalHeaders": {
    "cache-control": "no-cache"
  },
  "routes": [
    {
      "route": "*.js",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "*.css",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ]
}